\mode<presentation>

\definecolor{beamer@zdsblue}{HTML}{084561}
\definecolor{beamer@zdsorange}{HTML}{F8AD32}
\definecolor{beamer@zdsbg}{HTML}{F7F7F7}

\setbeamercolor{structure}{fg=beamer@zdsblue}

\setbeamercolor{item}{fg=beamer@zdsorange, bg=white}
\setbeamercolor{subitem}{fg=beamer@zdsorange, bg=white}
\setbeamercolor{subsubitem}{fg=beamer@zdsorange, bg=white}

\setbeamercolor{titlelike}{bg=beamer@zdsblue,fg=white}
\setbeamercolor{footline}{bg=beamer@zdsblue,fg=white}

\setbeamercolor{title}{fg=white,bg=beamer@zdsblue}
\setbeamercolor{subtitle}{fg=white,bg=beamer@zdsblue}
\setbeamercolor{author}{fg=white,bg=beamer@zdsblue}
\setbeamercolor{date}{fg=white,bg=beamer@zdsblue}

\setbeamercolor{title in head/foot}{fg=white,bg=beamer@zdsblue}
\setbeamercolor{author in head/foot}{fg=white,bg=beamer@zdsblue}
\setbeamercolor{date in head/foot}{fg=white,bg=beamer@zdsblue}
\setbeamercolor{section in head/foot}{fg=white,bg=beamer@zdsblue}
\setbeamercolor{subsection in head/foot}{fg=white,bg=beamer@zdsblue}

\setbeamercolor{section in toc}{fg=white,bg=beamer@zdsblue}
\setbeamercolor{section in toc shaded}{fg=white,bg=beamer@zdsblue}

\setbeamercolor{normal text}{fg=black}
\setbeamercolor{alterted text}{fg=beamer@zdsorange}
\setbeamercolor{example text}{fg=beamer@zdsblue}

\setbeamercolor{background canvas}{bg=beamer@zdsbg}

\mode
<all>
